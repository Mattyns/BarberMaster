function abrirPopup() {
      // URL do link
      const url = "https://calendar.app.google/DqmdxCBghHs8K5i99";

      // Configuração da janela
      const largura = 600;
      const altura = 400;
      const left = (screen.width - largura) / 2; // Centralizar na tela
      const top = (screen.height - altura) / 2;

      // Abrir o popup
      window.open(
        url, 
        "popupWindow", 
        `width=${largura},height=${altura},top=${top},left=${left},resizable=yes,scrollbars=yes`
      );
    }

    